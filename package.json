{
  "name": "@vlzh/nest-typeorm-i18n",
  "version": "0.0.2",
  "description": "Fork of @nestjs/typeorm for compatibility with typeorm-i18n",
  "author": "VLZH",
  "license": "MIT",
  "url": "https://github.com/VLZH/nest-typeorm-i18n",
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.json",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "test:integration": "jest --config ./tests/jest-e2e.json --runInBand"
  },
  "devDependencies": {
    "@nestjs/common": "6.10.1",
    "@nestjs/core": "6.10.1",
    "@nestjs/platform-express": "6.10.1",
    "@nestjs/testing": "6.10.1",
    "@types/jest": "24.0.23",
    "@types/node": "7.10.8",
    "@types/supertest": "2.0.8",
    "@types/uuid": "3.4.6",
    "husky": "3.1.0",
    "jest": "24.9.0",
    "lint-staged": "9.4.3",
    "mysql": "2.17.1",
    "pg": "7.14.0",
    "prettier": "1.19.1",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.5.3",
    "rxjs-compat": "6.5.3",
    "supertest": "4.0.2",
    "ts-jest": "24.1.0",
    "typeorm-i18n": "^0.0.6",
    "typescript": "3.7.2"
  },
  "dependencies": {},
  "peerDependencies": {
    "@nestjs/common": "^6.7.0",
    "@nestjs/core": "^6.7.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.0.0"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/VLZH/nest-typeorm-i18n"
  }
}
