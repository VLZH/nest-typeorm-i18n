{
  "name": "@vlzh/nest-typeorm-i18n",
  "version": "0.0.2",
  "description": "Fork of @nestjs/typeorm for compatibility with typeorm-i18n",
  "author": "VLZH",
  "license": "MIT",
  "url": "https://github.com/VLZH/nest-typeorm-i18n",
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.json",
    "format": "prettier --write \"**/*.ts\"",
    "lint": "eslint 'lib/**/*.ts' --fix",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "test:e2e": "jest --config ./tests/jest-e2e.json --runInBand",
    "test:e2e:dev": "jest --config ./tests/jest-e2e.json --runInBand --watch",
    "prerelease": "npm run build",
    "release": "release-it"
  },
  "devDependencies": {
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-angular": "8.3.4",
    "@nestjs/common": "7.1.3",
    "@nestjs/core": "7.1.3",
    "@nestjs/platform-express": "7.1.3",
    "@nestjs/testing": "7.1.3",
    "@types/jest": "26.0.0",
    "@types/node": "7.10.8",
    "@types/supertest": "2.0.9",
    "@types/uuid": "8.0.0",
    "@typescript-eslint/eslint-plugin": "3.2.0",
    "@typescript-eslint/parser": "3.2.0",
    "eslint": "7.2.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.21.2",
    "husky": "4.2.5",
    "jest": "26.0.1",
    "lint-staged": "10.2.9",
    "mysql": "2.18.1",
    "pg": "8.2.1",
    "prettier": "2.0.5",
    "reflect-metadata": "0.1.13",
    "release-it": "13.6.2",
    "rxjs": "6.5.5",
    "rxjs-compat": "6.5.5",
    "supertest": "4.0.2",
    "ts-jest": "26.1.0",
    "typeorm-i18n": "^0.0.6",
    "typescript": "3.9.5"
  },
  "dependencies": {
    "uuid": "8.1.0"
  },
  "peerDependencies": {
    "@nestjs/common": "^6.7.0 || ^7.0.0",
    "@nestjs/core": "^6.7.0 || ^7.0.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.0.0"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -c .commitlintrc.json -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/VLZH/nest-typeorm-i18n"
  }
}
